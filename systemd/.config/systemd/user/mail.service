[Unit]
Description=Sync emails
Documentation=https://github.com/tong/dot/README.md
After=network-online.target
Wants=network-online.target

[Service]
#User=
ExecStart=/home/tong/.local/bin/mailsync
Restart=on-failure
RestartSec=60
; Restart=always
; RestartSec=360s
; TimeoutStopSec=15

; [Install] 
; WantedBy= multi-user.target

[Install]
WantedBy=default.target
